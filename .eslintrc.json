{
	"parserOptions": {
		"parser": "@typescript-eslint/parser",
		"sourceType": "module",
		"ecmaVersion": 2022
	},
	"env": {
		"node": true,
		"es6": true
	},
	"extends": ["airbnb/base", "plugin:@typescript-eslint/recommended", "prettier"],
	"plugins": ["@typescript-eslint"],
	"rules": {
		"linebreak-style": 0,
		"arrow-parens": 0,
		"max-len": 0,
		"import/no-extraneous-dependencies": 0
	}
}
